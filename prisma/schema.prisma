generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Subscription {
  id              String    @id @default(cuid())
  userId          String
  serviceName     String
  startDate       DateTime
  trialEndDate    DateTime?
  category        String
  cost            Float
  calendarEventId String?
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
}
